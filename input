Opipeline{ 
 agent any
 stages{
  stage('Example'){
   input{ 
    message 'should we continue'
    ok 'yes we should'
    submitter 'manoj'
    parameter{
     string(name:'PERSON',defaultValue:'mr jenkins',description:'hello all this is desc')
    }
  }
  steps{
   echo ' hello ${PERSON}'
  }
}
}
}
